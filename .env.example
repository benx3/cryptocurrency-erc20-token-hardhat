# Đổi tên file này thành .env rồi điền các giá trị bên dưới

# API key từ Infura (project dùng mạng Sepolia)
INFURA_API_KEY=YOUR_INFURA_API_KEY_HERE

# Private key của ví MetaMask (tài khoản dùng cho testnet, KHÔNG dùng ví chính có tiền thật)
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# (Tùy chọn) API key dùng để verify contract trên Etherscan (Sepolia)
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY_HERE
